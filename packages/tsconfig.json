{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": false,
    "experimentalDecorators": true,
    "lib": ["es2015", "es6", "es7", "es2017.object", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es2016"
  }
}

  "compilerOptions": {
    "declaration": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "target": "es2016",
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "lib": ["es2015", "es6", "es7", "es2017.object", "dom"]
  }
}
